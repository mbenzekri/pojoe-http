{
    "name": "pojoe-http",
    "version": "1.0.0",
    "title": "http Pojoe steps",
    "description": "this module provides Pojoe steps for http/https calls (uses got)",
    "main": "./node_modules/pojoe/steps/pojoetest.js",
    "config": {
        "steps": [
            "HttpDownload"
        ]
    },
    "scripts": {
        "build": "tsc",
        "predoc": "tsc",
        "doc": "node ./node_modules/pojoe/steps/pojoedoc",
        "pretest": "npm run doc",
        "test": "node ./node_modules/pojoe/steps/pojoetest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mbenzekri/pojoe-http.git"
    },
    "author": "Mohamed Ben Zekri",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/mbenzekri/pojoe-http/issues"
    },
    "homepage": "https://github.com/mbenzekri/pojoe-http#readme",
    "dependencies": {
        "got": "^11.8.5",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/got": "^9.4.4",
        "@types/node": "^12.0.1"
    },
    "peerDependencies": {
        "pojoe": "github:mbenzekri/pojoe"
    }
}
